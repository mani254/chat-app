{
  "name": "@workspace/database",
  "version": "0.0.1",
  "description": "Database schemas and types for MongoDB",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "author": "mani254",
  "keywords": [
    "openchat",
    "chat",
    "mongodb",
    "mongoose",
    "schemas",
    "typescript"
  ],
  "scripts": {
    "clean": "rimraf dist"
  },
  "dependencies": {
    "bcryptjs": "^3.0.2",
    "mongoose": "^8.6.2",
    "slugify": "^1.6.6"
  },
  "devDependencies": {
    "@types/mongoose": "^5.11.96",
    "@workspace/typescript-config": "workspace:*",
    "rimraf": "^6.0.1",
    "typescript": "^5.8.2"
  },
  "exports": {
    ".": {
      "types": "./src/index.ts",
      "default": "./src/index.ts"
